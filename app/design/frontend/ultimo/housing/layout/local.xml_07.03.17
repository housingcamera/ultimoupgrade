				<label>Page Header, Primary, Left Column</label>
				<action method="setElementClass"><value>item container_header_primary_left_1 std</value></action>
			</block>

			<block type="page/html_wrapper" name="container_header_primary_central_1">
				<label>Page Header, Primary, Central Column</label>
				<action method="setElementClass"><value>item container_header_primary_central_1 std</value></action>
			</block>

			<block type="page/html_wrapper" name="container_header_primary_right_1">
				<label>Page Header, Primary, Right Column</label>
				<action method="setElementClass"><value>item container_header_primary_right_1 std</value></action>
			</block>
			
		</reference>


		<!-- Inside main menu bar ====================================================================== -->
		<reference name="catalog.topnav.menu">

			<!-- Insert sticky logo into the menu -->
			<action method="insert" ifconfig="ultimo/header/sticky_logo">
				<block>catalog.topnav.stickylogo</block>
				<siblingName></siblingName>
				<after>0</after>
				<alias>stickylogo</alias>
			</action>

		</reference>
		
		
		<!-- Top links (user menu) ====================================================================== -->
		<reference name="top.links">

			<!-- Remove checkout links -->
			<remove name="checkout_cart_link" />
			
		</reference>
		
		
