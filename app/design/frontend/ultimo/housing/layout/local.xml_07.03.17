	<!-- 3 COLUMN PAGES %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
	<page_three_columns>
	
		<reference name="right">
			<action method="unsetChild"><name>paypal.partner.right.logo</name></action>
		</reference>
		
		<reference name="left">
