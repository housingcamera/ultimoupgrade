	<!-- PRODUCT VIEW %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
	<catalog_product_view>
	
		<!-- Change default template for product view. IMPORTANT: the same as in 'review_product_list'. -->
		<reference name="root">
			<action method="setTemplate"><template>page/1column.phtml</template></action>
		</reference>

		<!-- Related products - add related products block to the "product.info" block and remove it from the column -->
		<reference name="right">
			<action method="unsetChild"><name>catalog.product.related</name></action>
		</reference>

		<reference name="product.info">
			<block type="page/html_wrapper" name="related_products_wrapper">
				<action method="setElementClass"><value>box-collateral box-related</value></action>
				<action method="insert">
					<block>catalog.product.related</block>
				</action>
			</block>
		</reference>

		<reference name="catalog.product.related">
			<action method="setTemplate"><template helper="ultimo/data/getRelatedProductsTemplate" /></action>
		</reference>

		<!-- Up-sell products -->
		<!-- Block with upsells is already added (by default) to "product.info" block -->
		<reference name="product.info">
			<block type="page/html_wrapper" name="upsell_products_wrapper">
				<action method="setElementClass"><value>box-collateral box-upsell</value></action>
				<action method="insert">
					<block>product.info.upsell</block>
				</action>
			</block>
		</reference>

		<reference name="product.info.upsell">
			<action method="setColumnCount"><columns>4</columns></action>
			<action method="setItemLimit"><type>upsell</type><limit>32</limit></action>
		</reference>

		<reference name="product.info">
		
			<!-- Change review summary template: removed links to review page. IMPORTANT: new template file. -->
			<action method="addReviewSummaryTemplate"><type>default</type><template>review/helper/summary_nolinks.phtml</template></action>
			
			<!-- Uncomment to restore Magento's default product image display template -->
			<!--<block type="catalog/product_view_media" name="product.info.media" as="media" template="catalog/product/view/media.phtml"/>-->

			<!-- Reviews -->
			<block type="page/html_pager" name="product_review_list.toolbar">
				<action method="setLimit"><limit>20</limit></action>
			</block>
			<block type="core/template" name="product_review_list.count" template="review/product/view/count.phtml" />
			<block type="review/product_view_list" name="product.reviews" as="reviews" template="review/product/view/list.phtml">
				<block type="review/form" name="product.review.form" as="review_form">
					<block type="page/html_wrapper" name="product.review.form.fields.before" as="form_fields_before" translate="label">
						<label>Review Form Fields Before</label>
						<action method="setMayBeInvisible"><value>1</value></action>
					</block>
				</block>
			</block>

			<!-- Tabs. They are displayed inside the block with product collateral data. -->
			<block type="catalog/product_view_tabs" name="product.info.tabs" as="info_tabs" template="catalog/product/view/tabs.phtml" >
				
				<!-- Product description -->
				<action method="addTab" translate="title" module="catalog"><alias>description</alias><title>Product ssssssssssssssssDescription</title><block>catalog/product_view_description</block><template>catalog/product/view/description.phtml</template></action>
				
				<!-- Recurring profile -->
				<action method="addTab" translate="title" module="payment"><alias>product.info.additional.recurring.schedule</alias><title>Recurring Profile</title><block>payment/catalog_product_view_profile</block><template>payment/catalog/product/view/profile/schedule.phtml</template></action>
				
				<!-- Product attributes -->
				<action method="addTab" translate="title" module="catalog"><alias>additional</alias><title>Specifications</title><block>catalog/product_view_attributes</block><template>catalog/product/view/attributes.phtml</template></action>
				
				<!-- Reviews-->
				<!-- Do not change 'tabreviews' alias. It is needed in JS scripts -->
				<action method="addTab" translate="title" module="review"
					ifconfig="ultimo/product_page/collateral_reviews"><alias>tabreviews</alias><title>Reviews</title><block>review/product_view_list</block><template>review/product/view/list.phtml</template></action>
				
				<!-- Tags -->
				<action method="addTab" translate="title" module="tag"
					ifconfig="ultimo/product_page/collateral_tags"><alias>tags</alias><title>Product Tags</title><block>tag/product_list</block><template>tag/list.phtml</template></action>

				<!-- Custom blocks -->
				<action method="addTab" translate="title" module="catalog"><alias>product_cms_block1</alias><title helper="ultimo/template_catalog_product_view/getCmsBlockTitle"><id>block_product_tab1</id></title><block>core/template</block><template>catalog/product/view/cms_block1.phtml</template></action>

				<action method="addTab" translate="title" module="catalog"><alias>product_cms_block2</alias><title helper="ultimo/template_catalog_product_view/getCmsBlockTitle"><id>block_product_tab2</id></title><block>core/template</block><template>catalog/product/view/cms_block2.phtml</template></action>

				<!-- Related products -->
				<action method="addTab" translate="title" module="catalog"
					ifconfig="ultimo/product_page/collateral_related"><alias>related</alias><title>Related Products</title><block>catalog/product_list_related</block><template helper="ultimo/data/getRelatedProductsTemplate" /></action>

				<!-- Up-sell products -->
				<action method="addTab" translate="title" module="catalog"
					ifconfig="ultimo/product_page/collateral_upsell"><alias>upsell</alias><title>Other Products</title><block>catalog/product_list_upsell</block><template>catalog/product/list/upsell.phtml</template></action>
				
			</block> <!-- end: tabs -->

			<!-- Container for product collateral data (displayed as tabs or stacked blocks) -->
			<block type="catalog/product_view" name="product.collaterals" as="productCollaterals" template="catalog/product/view/collateral.phtml">

				<!-- Append tabs (by block name) -->
				<action method="append"><block>product.info.tabs</block></action>

				<!-- Product description -->
				<block type="page/html_wrapper" name="product_description_wrapper">
					<action method="addToParentGroup"><group>detailed_info</group></action>
					<action method="setElementClass"><value>box-collateral box-description</value></action>
					<action method="insert">
						<block>product.description</block>
					</action>
				</block>

				<!-- Recurring profile -->
				<block type="page/html_wrapper" name="recurring_info_wrapper">
					<action method="addToParentGroup"><group>detailed_info</group></action>
					<action method="setElementClass"><value>box-collateral box-recurring std</value></action>
					<block type="payment/catalog_product_view_profile" name="product.info.additional.recurring.schedule" as="recurring_info" template="payment/catalog/product/view/profile/schedule.phtml" />
				</block>

				<!-- Product attributes -->
				<block type="page/html_wrapper" name="product_attributes_wrapper">
					<action method="addToParentGroup"><group>detailed_info</group></action>
					<action method="setElementClass"><value>box-collateral box-additional</value></action>
					<action method="insert">
						<block>product.attributes</block>
					</action>
				</block>

				<!-- Reviews-->
				<action method="append"><block>product.reviews</block></action>

				<!-- Tags -->
				<action method="append"><block>product_tag_list</block></action>

				<!-- Custom blocks -->
				<block type="page/html_wrapper" name="block_product_tab1_wrapper" as="product_cms_block1">
					<action method="addToParentGroup"><group>detailed_info</group></action>
					<action method="setElementClass"><value>box-collateral box-custom-block std</value></action>
					<block type="cms/block" name="block_product_tab1">
						<action method="setBlockId"><block_id>block_product_tab1</block_id></action>
					</block>
				</block>
				<block type="page/html_wrapper" name="block_product_tab2_wrapper" as="product_cms_block2">
					<action method="addToParentGroup"><group>detailed_info</group></action>
					<action method="setElementClass"><value>box-collateral box-custom-block std</value></action>
					<block type="cms/block" name="block_product_tab2">
						<action method="setBlockId"><block_id>block_product_tab2</block_id></action>
					</block>
				</block>

				<!-- Related products -->
				<action method="append"><block>related_products_wrapper</block></action>

				<!-- Up-sell products -->
				<action method="append"><block>upsell_products_wrapper</block></action>

				<!-- Example: how to add custom block which will be rendered by custom template file: -->
				<!--<block type="catalog/product_view" name="my_example" template="catalog/product/my_template.phtml">
					<action method="addToParentGroup"><group>detailed_info</group></action>
					<action method="setTitle" translate="title"><title>My block title</title></action>
				</block>-->

			</block> <!-- end: collateral -->

			<!-- Custom static blocks and block containers on product page -->

			<block type="page/html_wrapper" name="container_product_primary_1">
				<label>Product View, Primary Column, Container 1</label>
				<action method="setElementClass"><value>container_product_primary_1 std</value></action>
			</block>

			<block type="page/html_wrapper" name="container_product_primary_2">
				<label>Product View, Primary Column, Container 2</label>

				<!-- Class "block_product_primary_bottom" left for backward compatibility -->
				<action method="setElementClass"><value>container_product_primary_2 std block_product_primary_bottom</value></action>
				
				<!-- For backward compatibility, load static block "block_product_primary_bottom" -->
				<block type="cms/block" name="i_block_product_primary_bottom">
					<action method="setBlockId"><block_id>block_product_primary_bottom</block_id></action>
				</block>
			</block>

			<block type="page/html_wrapper" name="container_product_secondary_1">
				<label>Product View, Secondary Column, Container 1</label>
				<action method="setElementClass"><value>container_product_secondary_1 std</value></action>
			</block>

			<block type="page/html_wrapper" name="container_product_secondary_2">
				<label>Product View, Secondary Column, Container 2</label>

				<!-- Class "block_product_secondary_bottom" left for backward compatibility -->
				<action method="setElementClass"><value>container_product_secondary_2 std block_product_secondary_bottom</value></action>

				<!-- For backward compatibility, load static block "block_product_secondary_bottom" -->
				<block type="cms/block" name="i_block_product_secondary_bottom">
					<action method="setBlockId"><block_id>block_product_secondary_bottom</block_id></action>
				</block>

				<!-- Insert brand logo -->
				<!--
				<action method="insert">
					<block>brands_brands</block><siblingName/><after/>
				</action>
				-->
			</block>

			<block type="page/html_wrapper" name="container_product_image_1">
				<label>Product View, Image Column, Below Images</label>

				<!-- Class "block_product_img_bottom" left for backward compatibility -->
				<action method="setElementClass"><value>container_product_image_1 block_product_img_bottom std</value></action>

				<!-- For backward compatibility, load static block "block_product_img_bottom" -->
				<block type="cms/block" name="i_block_product_img_bottom">
					<action method="setBlockId"><block_id>block_product_img_bottom</block_id></action>
				</block>
			</block>

			<block type="cms/block" name="block_product_replace_related">
				<action method="setBlockId"><block_id>block_product_replace_related</block_id></action>
			</block>
			<block type="cms/block" name="block_product_replace_upsell">
				<action method="setBlockId"><block_id>block_product_replace_upsell</block_id></action>
			</block>

			<!-- Example: how to add custom static block to the product page.
				 Block needs to be loaded in product page template file with the following code:
				 echo $this->getChildHtml('my_unique_block_identifier');
			-->
			<!--
			<block type="cms/block" name="my_unique_block_identifier">
				<action method="setBlockId"><block_id>identifier_of_my_custom_static_block</block_id></action>
			</block>
			-->

		</reference> <!-- end: product.info -->

		<!-- Tags: additional actions.
			 Block with tags is originally added to "product.info.additional", so if we assign it to the group of collateral data,
			 we also need to remove it from "product.info.additional". -->
		<reference name="product_tag_list">
			<action method="addToParentGroup"
				ifconfig="ultimo/product_page/collateral_tags"><group>detailed_info</group></action>
		</reference>
		<reference name="product.info.additional">
			<action method="unsetChild"
				ifconfig="ultimo/product_page/collateral_tags"><name>product_tag_list</name></action>
		</reference>

		<!-- Reviews: additional actions.
			 Block with reviews is already added to "product.info", so if we assign it to the group of collateral data, we need to remove it from "product.info.additional". -->
		<reference name="product.reviews">
			<action method="addToParentGroup"
				ifconfig="ultimo/product_page/collateral_reviews"><group>detailed_info</group></action>
		</reference>
		<reference name="product.info">
			<action method="unsetChild"
				ifconfig="ultimo/product_page/collateral_reviews"><name>reviews</name></action> <!-- Remove by alias! -->
		</reference>

		<!-- Remove if tabs enabled.
			 Second part required to add reviews to tabs. See the first part in "info_tabs" block. -->
		<reference name="tabreviews">
			<block type="review/form" name="product.review.form" as="review_form">
				<block type="page/html_wrapper" name="product.review.form.fields.before" as="form_fields_before" translate="label">
					<label>Review Form Fields Before</label>
					<action method="setMayBeInvisible"><value>1</value></action>
				</block>
			</block>
		</reference>

		<!-- Related products: additional actions.
			 Block with related products is already added to "product.info", so if we assign it to the group of collateral data,
			 we also need to remove it from "product.info". -->
		<reference name="related_products_wrapper">
			<action method="addToParentGroup"
				ifconfig="ultimo/product_page/collateral_related"><group>detailed_info</group></action>
		</reference>
		<reference name="product.info">
			<action method="unsetChild"
				ifconfig="ultimo/product_page/collateral_related"><name>related_products_wrapper</name></action>
		</reference>

		<!-- Up-sells products - additional actions.
			 Block with upsells is originally added to "product.info", so if we assign it to the group of collateral data,
			 we also need to remove it from "product.info". -->
		<reference name="upsell_products_wrapper">
			<action method="addToParentGroup"
				ifconfig="ultimo/product_page/collateral_upsell"><group>detailed_info</group></action>
		</reference>
		<reference name="product.info">
			<action method="unsetChild"
				ifconfig="ultimo/product_page/collateral_upsell"><name>upsell_products_wrapper</name></action> <!-- Remove by alias! -->
		</reference>

		<!-- Remove "add to wishlist/compare" links from options box. It is already inserted in product view. -->
		<reference name="product.info.options.wrapper.bottom">
			<action method="unsetChild"><name>product.info.addto</name></action>
		</reference>

		<!-- Alerts -->
		<reference name="productalert.price">
			<action method="setIconName"><value>alarm</value></action>
		</reference>

		<reference name="productalert.stock">
			<action method="setIconName"><value>alarm</value></action>
		</reference>

		<!-- Container for configurable swatches module -->
		<reference name="product.info.media">
			<block type="core/text_list" name="product.info.media.after" as="after" />
		</reference>

	</catalog_product_view>


	<!-- CATEGORY %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
	
	<!-- Default category (without layered navigation) -->
	<catalog_category_default>
	
		<reference name="root">
			<action method="setTemplate"><template>page/2columns-left.phtml</template></action>
		</reference>

		<reference name="product_list">

			<!-- Custom static blocks -->
			<block type="cms/block" name="block_category_above_empty_collection">
				<action method="setBlockId"><block_id>block_category_above_empty_collection</block_id></action>
			</block>
			<block type="cms/block" name="block_category_below_collection">
				<action method="setBlockId"><block_id>block_category_below_collection</block_id></action>
			</block>
			
		</reference>
		
		<!-- Remove currency switcher from sidebar -->
		<reference name="left">
			<action method="unsetChild"><name>currency</name></action>
		</reference>

	</catalog_category_default>

	<!-- Category with layered navigation -->
	<catalog_category_layered>
	
		<reference name="root">
			<action method="setTemplate"><template>page/2columns-left.phtml</template></action>
		</reference>
		
		<reference name="product_list">
		
			<!-- Custom static blocks -->
			<block type="cms/block" name="block_category_above_empty_collection">
				<action method="setBlockId"><block_id>block_category_above_empty_collection</block_id></action>
			</block>
			<block type="cms/block" name="block_category_below_collection">
				<action method="setBlockId"><block_id>block_category_below_collection</block_id></action>
			</block>
			
		</reference>

		<!-- Remove currency switcher from sidebar -->
		<reference name="left">
			<action method="unsetChild"><name>currency</name></action>			
		</reference>
		
	</catalog_category_layered>
	
	
	<!-- CART %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
	<checkout_cart_index>

		<!-- Custom static blocks -->
		<reference name="checkout.cart">
		
			<block type="cms/block" name="block_cart_below_table">
				<action method="setBlockId"><block_id>block_cart_below_table</block_id></action>
			</block>
			<block type="cms/block" name="block_cart_below_totals">
				<action method="setBlockId"><block_id>block_cart_below_totals</block_id></action>
			</block>
			
		</reference>
		
	</checkout_cart_index>
	
		<!-- CHECKOUT %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
	<checkout_onepage_index>
		
		<!-- Block below progress block -->
		<reference name="right">
			<block type="page/html_wrapper" name="i_block_opc_below_progress">
				<action method="setElementClass"><value>block block_opc_below_progress std</value></action>
				<block type="cms/block" name="block_opc_below_progress" after="checkout.progress.wrapper">
					<action method="setBlockId"><block_id>block_opc_below_progress</block_id></action>
				</block>
			</block>
		</reference>
	
	</checkout_onepage_index>


	<!-- SEARCH %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
	
	<!-- Quick search -->
	<catalogsearch_result_index>
	
		<reference name="root">
			<action method="setTemplate"><template>page/2columns-left.phtml</template></action>
		</reference>
		
		<reference name="left">
			<!-- Keep layered navigation always on top. Replace: after="currency" with: before="-" -->
			<!--<action method="unsetChild"><name>catalogsearch.leftnav</name></action>
			<block type="catalogsearch/layer" name="catalogsearch.leftnav" before="-" template="catalog/layer/view.phtml"/>-->
			
			<!-- Remove currency switcher from sidebar -->
			<action method="unsetChild"><name>currency</name></action>
		</reference>
	
	</catalogsearch_result_index>
	
	<!-- Advanced search -->
	<catalogsearch_advanced_index>
	
		<!-- Remove (right) currency switcher from sidebar -->
		<reference name="left">
			<action method="unsetChild"><name>right_currency</name></action>
		</reference>
		
	</catalogsearch_advanced_index>
	
	<!-- Advanced search results -->
	<catalogsearch_advanced_result>
		
		<!-- Remove (right) currency switcher from RIGHT sidebar -->
		<reference name="right">
			<action method="unsetChild"><name>right_currency</name></action>
		</reference>
		
	</catalogsearch_advanced_result>

	<tag_product_list>

		<reference name="root">
			<action method="setTemplate"><template>page/2columns-left.phtml</template></action>
		</reference>
		
	</tag_product_list>


	<!-- REVIEWS %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
	<review_product_list>

		<!-- IMPORTANT: the same as 'catalog_product_view'. -->
		<reference name="root">
			<action method="setTemplate"><template>page/1column.phtml</template></action>
		</reference>
		
	</review_product_list>


	<!-- WISHLIST %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
	<wishlist_index_configure>

		<!-- Remove links from options wrapper on wishlist product update -->
		<reference name="product.info.options.wrapper.bottom">
			<action method="unsetChild"><name>product.info.addto</name></action>
		</reference>

	</wishlist_index_configure>


	<!-- CUSTOMER ACCOUNT %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
	<customer_account>

		<reference name="root">
			<action method="addBodyClass"><className>customer-account</className></action>
		</reference>

		<reference name="left">
			<!-- Remove cart from the left column -->
			<action method="unsetChild"><name>cart_sidebar</name></action>
			
			<!-- Remove Compare block, it is already inserted in DEFAULT section -->
			<action method="unsetChild"><name>catalog.compare.sidebar</name></action>
		</reference>

	</customer_account>


	<!-- CMS PAGES %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
	
	<!-- Remove static blocks displayed on CMS pages from these pages: home page, 404 no route 
		 IMPORTANT: make sure that you use correct ID -->
	<cms_index_noroute>
		<remove name="block_right_top"/>
		<remove name="block_right_bottom"/>
	</cms_index_noroute>
	<cms_index_defaultnoroute>
		<remove name="block_right_top"/>
		<remove name="block_right_bottom"/>
	</cms_index_defaultnoroute>

	<!-- Uncomment to allow MAP functionality on home page -->
	<!--
	<cms_index_index>
		<update handle="MAP_popup" />
	</cms_index_index>
	-->
	
	
	<!-- HOME PAGE %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
	<cms_index_index>
		
		<!-- Left column ====================================================================== -->
		<reference name="left">
			<block type="core/text_list" name="home_left" as="home_left" translate="label" before="-">
				<label>Home, Left Column</label>
			</block>
			
			<!-- Disable left sidebar blocks on home page -->
			<action ifconfig="ultimo/sidebar_blocks_home/compare" method="unsetChild"><name>catalog.compare.sidebar</name></action>
			<action ifconfig="ultimo/sidebar_blocks_home/compared" method="unsetChild"><name>right.reports.product.compared</name></action>
			<action ifconfig="ultimo/sidebar_blocks_home/viewed" method="unsetChild"><name>left.reports.product.viewed</name></action>
			
			<action ifconfig="ultimo/sidebar_blocks_home/wishlist" method="unsetChild"><name>wishlist</name></action>
			<action ifconfig="ultimo/sidebar_blocks_home/poll" method="unsetChild"><name>right.poll</name></action>
			<action ifconfig="ultimo/sidebar_blocks_home/tags" method="unsetChild"><name>tags_popular</name></action>
			
			<action ifconfig="ultimo/sidebar_blocks_home/paypal" method="unsetChild"><name>paypal.partner.right.logo</name></action>
			<action ifconfig="ultimo/sidebar_blocks_home/block_left_top" method="unsetChild"><name>block_left_top</name></action>
			<action ifconfig="ultimo/sidebar_blocks_home/block_left_bottom" method="unsetChild"><name>block_left_bottom</name></action>
		</reference>
		
		<!-- Deprecated -->
		<reference name="home_left">
			<block type="cms/block" name="block_home_left1">
				<action method="setBlockId"><block_id>block_home_left1</block_id></action>
			</block>
			<block type="cms/block" name="block_home_left2">
				<action method="setBlockId"><block_id>block_home_left2</block_id></action>
			</block>
			<block type="cms/block" name="block_home_left3">
				<action method="setBlockId"><block_id>block_home_left3</block_id></action>
			</block>
		</reference>
		
		<!-- Right column ====================================================================== -->
		<reference name="right">
			<block type="core/text_list" name="home_right" as="home_right" translate="label" before="-">
				<label>Home, Right Column</label>
			</block>
			
			<!-- Disable right sidebar blocks on home page -->
			<action ifconfig="ultimo/sidebar_blocks_home/paypal" method="unsetChild"><name>paypal.partner.right.logo</name></action>
			<action ifconfig="ultimo/sidebar_blocks_home/block_right_top" method="unsetChild"><name>block_right_top</name></action>
			<action ifconfig="ultimo/sidebar_blocks_home/block_right_bottom" method="unsetChild"><name>block_right_bottom</name></action>
		</reference>
		
		<!-- Deprecated -->
		<reference name="home_right">
			<block type="cms/block" name="block_home_right1">
				<action method="setBlockId"><block_id>block_home_right1</block_id></action>
			</block>
			<block type="cms/block" name="block_home_right2">
				<action method="setBlockId"><block_id>block_home_right2</block_id></action>
			</block>
			<block type="cms/block" name="block_home_right3">
				<action method="setBlockId"><block_id>block_home_right3</block_id></action>
			</block>
		</reference>

	</cms_index_index>

</layout>