	<!-- PRODUCT TYPES %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% -->
	<PRODUCT_TYPE_simple>
		<reference name="product.info.options.wrapper.bottom">
			<remove name="product.clone_prices"/>
		</reference>
	</PRODUCT_TYPE_simple>

	<PRODUCT_TYPE_configurable>
		<reference name="product.info.options.wrapper.bottom">
