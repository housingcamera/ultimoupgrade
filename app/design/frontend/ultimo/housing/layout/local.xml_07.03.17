			</block>
			<!--<block type="core/text_list" name="main_top" as="main_top" translate="label">
				<label>Preface, Full-width</label>
			</block>-->
			<block type="core/text_list" name="preface" as="preface" translate="label">
				<label>Preface</label>
			</block>
			<block type="core/text_list" name="postscript" as="postscript" translate="label">
				<label>Postscript</label>
			</block>
			<block type="core/text_list" name="main_bottom" as="main_bottom" translate="label">
				<label>Postscript, Full Width</label>
			</block>

			<!-- Sticky logo which can be optionally inserted to the sticky header -->
			<block type="core/template" name="catalog.topnav.stickylogo" as="stickylogo" template="page/html/logo_sticky.phtml" />
		</reference>
		
		
		<!-- Remove sidebar blocks ====================================================================== -->
		
		<!-- Uncomment these tags to remove selected blocks -->
