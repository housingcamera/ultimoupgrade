			<block type="cms/block" name="block_right_top" before="-">
				<action method="setBlockId"><block_id>block_right_top</block_id></action>
			</block>
			<block type="cms/block" name="block_right_bottom" after="-">
				<action method="setBlockId"><block_id>block_right_bottom</block_id></action>
			</block>

			<!-- Important: add at the very end -->
			<block type="core/text_list" name="right_top" as="right_top" translate="label" before="-">
				<label>Right Column, Top</label>
			</block>
			
		</reference>
		

		<!-- Left ====================================================================== -->
		<reference name="left">
			
			<remove name="left.newsletter"/> <!-- Moved newsletter to the footer -->
			<action method="unsetChild"><name>tags_popular</name></action>

			<block type="catalog/product_compare_sidebar" name="catalog.compare.sidebar" template="catalog/product/compare/sidebar.phtml"/>
			<block type="reports/product_compared" name="right.reports.product.compared" template="reports/product_compared.phtml" />
			<block type="reports/product_viewed" name="left.reports.product.viewed" template="reports/product_viewed.phtml" />
			<block type="wishlist/customer_sidebar" name="wishlist_sidebar" as="wishlist" template="wishlist/sidebar.phtml"/>
			<block type="poll/activePoll" name="right.poll">
				<action method="setPollTemplate"><template>poll/active.phtml</template><type>poll</type></action>
				<action method="setPollTemplate"><template>poll/result.phtml</template><type>results</type></action>
			</block>
			<block type="tag/popular" name="tags_popular" template="tag/popular.phtml"/>
			<!--<block type="paypal/logo" name="paypal.partner.right.logo" template="paypal/partner/logo.phtml"/>-->
			
			<!-- Deprecated: custom static blocks -->
			<block type="cms/block" name="block_left_top" before="-">
				<action method="setBlockId"><block_id>block_left_top</block_id></action>
			</block>
			<block type="cms/block" name="block_left_bottom" after="-">
				<action method="setBlockId"><block_id>block_left_bottom</block_id></action>
			</block>

			<!-- Remove optionally -->
			<action ifconfig="ultimo/header/compare" method="unsetChild"><name>catalog.compare.sidebar</name></action>

			<!-- Important: add at the very end -->
			<block type="core/text_list" name="left_top" as="left_top" translate="label" before="-">
				<label>Left Column, Top</label>
			</block>
			
		</reference>


		<!-- Header ====================================================================== -->
		<reference name="header">

			<!-- Welcome message displayed as a block (added after Magento 1.7.0.2) -->
			<block type="page/html_welcome" name="welcome" as="welcome"/>

			<!-- Add currency switcher to the header -->
			<block type="directory/currency" name="currency" template="directory/currency.phtml"/>

			<!-- Site logo -->
			<block type="page/html_header" name="logo" template="page/html/logo.phtml" />

			<!-- Search box wrapper -->
			<block type="page/html_header" name="search_wrapper" template="page/html/search_wrapper.phtml">

				<action method="insert">
					<block>top.search</block>
				</action>
				
			</block>
		
			<!-- Mini cart -->
			<block type="checkout/cart_sidebar" name="cart_sidebar" template="checkout/cart/mini.phtml" before="-">

				<action method="addItemRender"><type>simple</type><block>checkout/cart_item_renderer</block><template>checkout/cart/sidebar/default.phtml</template></action>
				<action method="addItemRender"><type>grouped</type><block>checkout/cart_item_renderer_grouped</block><template>checkout/cart/sidebar/default.phtml</template></action>
				<action method="addItemRender"><type>configurable</type><block>checkout/cart_item_renderer_configurable</block><template>checkout/cart/sidebar/default.phtml</template></action>
				<block type="core/text_list" name="cart_sidebar.extra_actions" as="extra_actions" translate="label" module="checkout">
					<label>Shopping Cart Sidebar Extra Actions</label>
				</block>
				<block type="cms/block" name="block_mini_cart_above_products">
					<action method="setBlockId"><block_id>block_mini_cart_above_products</block_id></action>
				</block>
				
			</block>

			<!-- Mini compare -->
			<block type="catalog/product_compare_sidebar" name="compare.mini" as="compareMini" template="catalog/product/compare/mini.phtml"/>

			<!-- Custom links in Top Links -->
			<block type="cms/block" name="block_header_top_links">
				<action method="setBlockId"><block_id>block_header_top_links</block_id></action>
			</block>

			<!-- Account links: container for Top Links and (optional) custom links -->
			<block type="page/html_header" name="account_links" template="page/html/account_links.phtml">

				<action method="insert">
					<block>top.links</block>
					<siblingName></siblingName>
					<after>0</after>
					<alias>topLinks</alias>
				</action>
				<action method="insert">
					<block>block_header_top_links</block>
				</action>

			</block>

			<!-- User menu: container for account links, search, Mini Cart, Mini Compare -->
			<block type="page/html_header" name="user_menu" template="page/html/user_menu.phtml">

				<!-- TODO: Remove un-needed blocks inserts -->
				<!-- <action method="insert">
					<block>search_wrapper</block>
				</action>
				<action method="insert">
					<block>account_links</block>
				</action>
				<action method="insert">
					<block>cart_sidebar</block>
				</action>
				<action method="insert">
					<block>compare.mini</block>
					<siblingName></siblingName>
					<after>0</after>
					<alias>compareMini</alias>
				</action> -->

			</block>

			<!-- Custom static blocks and block containers: -->

			<!-- Header top -->

			<block type="core/text_list" name="container_mobile_header_top" as="container_mobile_header_top" translate="label" before="-">
				<label>Page Mobile Header, Top</label>
			</block>

			<block type="page/html_wrapper" name="container_header_top_left_1">
				<label>Page Header, Top, Left</label>
				<action method="setElementClass"><value>item item-left container_header_top_left_1 std</value></action>
			</block>

			<block type="page/html_wrapper" name="container_header_top_right_1">
				<label>Page Header, Top, Right</label>
				<action method="setElementClass"><value>item item-right container_header_top_right_1 std</value></action>
			</block>

			<block type="page/html_wrapper" name="container_header_top_central_1">
				<label>Page Header, Top, Central</label>
				<action method="setElementClass"><value>item item-right container_header_top_central_1 std</value></action>
			</block>

			<!-- Deprecated -->
			<block type="cms/block" name="block_header_top_left">
				<action method="setBlockId"><block_id>block_header_top_left</block_id></action>
			</block>
			<block type="cms/block" name="block_header_top_left2">
				<action method="setBlockId"><block_id>block_header_top_left2</block_id></action>
			</block>
			<block type="cms/block" name="block_header_top_right">
				<action method="setBlockId"><block_id>block_header_top_right</block_id></action>
			</block>
			<block type="cms/block" name="block_header_top_right2">
